{
    "name": "hamedrajabpour/laravel-tags",
    "description": "Add tags and taggable behaviour to your Laravel app",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hamedrajabpour\\Tags\\": "src/"
        }
    },
    "authors": [
        {
            "name": "hamed rajabpour",
            "email": "hamedrajabpoor724@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.67|^9.0|^10.0|^11.0",
        "nesbot/carbon": "^2.63",
        "spatie/eloquent-sortable": "^3.10|^4.0",
        "spatie/laravel-package-tools": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13|^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.22|^2.0",
        "phpunit/phpunit": "^9.5.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Hamedrajabpour\\Tags\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hamedrajabpour\\Tags\\TagsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
